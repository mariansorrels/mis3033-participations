<Window x:Class="GOTquote.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GOTquote"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,0,-6">
        <Grid.Background>
            <ImageBrush ImageSource="https://gayexpress.co.nz/wp-content/uploads/2019/04/Maisie-online.jpg"/>
        </Grid.Background>
        <Button x:Name="btngetquote" Content="Get Quote" HorizontalAlignment="Left" Margin="78,260,0,0" VerticalAlignment="Top" Background="#FF1C2537" Foreground="#FFFEFBFB" Click="btngetquote_Click" Height="30" Width="154" FontFamily="Algerian" FontSize="20"/>
        <Label x:Name="lblquote" Content="" HorizontalAlignment="Left" Margin="78,299,0,0" VerticalAlignment="Top" Background="#FF35405E" Height="90" Width="637" FontFamily="Book Antiqua" FontWeight="Bold" Foreground="#FFFCF5F5"/>
    </Grid>
</Window>
